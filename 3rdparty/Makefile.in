# NOTE: The `--with-zlib` flag works differently between Mesos and
# Protobuf. Protobuf uses `--with-zlib` as an on/off switch for data
# compression instead of a path specifier for ZLib, so we need to set up
# the `CPPFLAGS` and `LDFLAGS` if `--with-zlib=DIR` is given.
@WITH_BUNDLED_PROTOBUF_TRUE@$(PROTOBUF)-build-stamp: $(PROTOBUF)-stamp
@WITH_BUNDLED_PROTOBUF_TRUE@	cd $(PROTOBUF) &&				\
@WITH_BUNDLED_PROTOBUF_TRUE@	  ./configure $(CONFIGURE_ARGS)			\
@WITH_BUNDLED_PROTOBUF_TRUE@	              CPPFLAGS="$(ZLIB_INCLUDE_FLAGS)"	\
@WITH_BUNDLED_PROTOBUF_TRUE@	              LDFLAGS="$(ZLIB_LINKER_FLAGS)" &&	\
@WITH_BUNDLED_PROTOBUF_TRUE@	  $(MAKE) $(AM_MAKEFLAGS)
@WITH_BUNDLED_PROTOBUF_TRUE@	touch $@
