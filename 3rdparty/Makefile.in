@WITH_BUNDLED_PROTOBUF_FALSE@	touch $@

@WITH_BUNDLED_GRPC_TRUE@$(LIB_GRPC): $(GRPC)-build-stamp

# NOTE: We pass compiler flags through environment variables rather than the
# command line because gRPC uses target-specific assignments to append compiler
# flags, which will be overwritten by the command line. See:
# https://www.gnu.org/software/make/manual/html_node/Target_002dspecific.html
#
# NOTE: We use EXTRA_CPPFLAGS to pass `-Wno-error`, because that one needs to be
# appended and not prepended to the constructed flags.
#
# NOTE: We compile gRPC with its embedded c-ares library to avoid a link error
# because Mesos is not aware of the library; see MESOS-9505. If bundle c-ares
# to resolve MESOS-9338 in the future, we should remove `HAS_SYSTEM_CARES` below
# and set up proper include and linker flags instead.
@WITH_BUNDLED_GRPC_TRUE@$(GRPC)-build-stamp: $(GRPC)-stamp			\
@WITH_BUNDLED_GRPC_TRUE@                     $(PROTOBUF)-build-stamp
@WITH_BUNDLED_GRPC_TRUE@	cd $(GRPC) &&					\
@WITH_BUNDLED_GRPC_TRUE@	  CPPFLAGS="$(PROTOBUF_INCLUDE_FLAGS)		\
@WITH_BUNDLED_GRPC_TRUE@	            $(SSL_INCLUDE_FLAGS)		\
@WITH_BUNDLED_GRPC_TRUE@	            $(ZLIB_INCLUDE_FLAGS)		\
@WITH_BUNDLED_GRPC_TRUE@	            $(CPPFLAGS)"			\
@WITH_BUNDLED_GRPC_TRUE@	  CFLAGS="$(CFLAGS)"				\
@WITH_BUNDLED_GRPC_TRUE@	  CXXFLAGS="$(CXXFLAGS)"			\
@WITH_BUNDLED_GRPC_TRUE@	  $(MAKE) $(AM_MAKEFLAGS)			\
@WITH_BUNDLED_GRPC_TRUE@	    $(LIB_GRPC:%=$(realpath $(abs_builddir))/%)	\
@WITH_BUNDLED_GRPC_TRUE@	    CC="$(CC)"					\
@WITH_BUNDLED_GRPC_TRUE@	    CXX="$(CXX)"				\
@WITH_BUNDLED_GRPC_TRUE@	    EXTRA_CPPFLAGS=-Wno-error			\
@WITH_BUNDLED_GRPC_TRUE@	    LD="$(CC)"					\
@WITH_BUNDLED_GRPC_TRUE@	    LDXX="$(CXX)"				\
@WITH_BUNDLED_GRPC_TRUE@	    LDFLAGS="$(PROTOBUF_LINKER_FLAGS)		\
@WITH_BUNDLED_GRPC_TRUE@	             $(SSL_LINKER_FLAGS)		\
@WITH_BUNDLED_GRPC_TRUE@	             $(ZLIB_LINKER_FLAGS)		\
@WITH_BUNDLED_GRPC_TRUE@	             $(LDFLAGS)"			\
@WITH_BUNDLED_GRPC_TRUE@	    LDLIBS="$(LDLIBS)"				\
@WITH_BUNDLED_GRPC_TRUE@	    HAS_PKG_CONFIG=false			\
@WITH_BUNDLED_GRPC_TRUE@	    HAS_SYSTEM_CARES=false			\
@WITH_BUNDLED_GRPC_TRUE@	    HAS_PROTOC=true				\
@WITH_BUNDLED_GRPC_TRUE@	    HAS_VALID_PROTOC=true			\
@WITH_BUNDLED_GRPC_TRUE@	    PROTOC="$(PROTOC)"
@WITH_BUNDLED_GRPC_TRUE@	touch $@
@WITH_BUNDLED_GRPC_FALSE@$(GRPC)-build-stamp:
@WITH_BUNDLED_GRPC_FALSE@	touch $@

